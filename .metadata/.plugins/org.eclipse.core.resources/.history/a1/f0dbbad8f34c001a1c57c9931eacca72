package cba.tcs.weather.parameters;

public class Condition {
	
	private enum WeatherCondition {
		SUNNY("Sunny"), RAINY("Rainy"), SNOW("Snow");

		private String name;

		WeatherCondition(String name) {
			this.name = name;
		}

		@Override
		public String toString() {
			return this.name;
		}
	};

}
